rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // match /{allPaths=**} {
    //   allow read;
    //   // allow read, write: if request.auth!=null;
    // }

    match /public/{userId}/{allPaths=**} {
      allow read;
      allow write: if userId == request.auth.uid
    }
  }
}
