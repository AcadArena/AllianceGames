{
  "name": "websocket",
  "version": "1.0.13",
  "description": "Websocket server used to communicate between the control and overlay",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env GOOGLE_APPLICATION_CREDENTIALS=./service-account-file.json  NODE_ENV=development nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "start": "cross-env GOOGLE_APPLICATION_CREDENTIALS=./service-account-file.json NODE_ENV=production node -r esbuild-register ./src/index.ts",
    "build": "cross-env GOOGLE_APPLICATION_CREDENTIALS=./service-account-file.json tsc",
    "lint": "tsc --noEmit && TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@fastify/cors": "8.1.0",
    "axios": "0.27.2",
    "dotenv": "16.0.2",
    "firebase-admin": "11.0.1",
    "interface": "*",
    "ip": "1.1.8",
    "socket.io": "4.5.2"
  },
  "devDependencies": {
    "@types/ip": "1.1.0",
    "@types/node": "18.7.16",
    "config": "*",
    "cross-env": "7.0.3",
    "esbuild": "0.15.7",
    "esbuild-register": "3.3.3",
    "eslint": "8.23.0",
    "nodemon": "2.0.19",
    "tsconfig": "*",
    "typescript": "4.8.3"
  }
}
